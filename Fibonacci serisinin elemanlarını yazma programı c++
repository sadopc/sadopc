//FİBONACCİ SERİSİ PROGRAMI

#include <iostream>
#include <stdlib.h>
#include <time.h>
#include <cmath>
using namespace std;
int main(){
    int a, b, c, n, i;
    b=1;
    c=1;
    cout << "fibonacci serisinin kaç elemanını görmek istersiniz (min 3 basamak) : ";
    cin >> n;
    if (n<=0){
        cout << "mal mısın amk";
    }
    else {
        cout << "1  1  ";
        for (i=0 ; i+2<n ; i++){
            a=b+c;
            cout << a << "  ";
            b=c;
            c=a;
    }
    }
}
